URxvt.termName:     xterm-256color

URxvt.font:         xft:Ricty for Powerline:size=12:antialias=true
URxvt.allow_bold:   True
! URxvt.font:size=12

URxvt.geometry:     149x50
URxvt.scrollBar:    false

URxvt.depth:        32
! URxvt.transparent:  false
URxvt.fading:       0


! URxvt.imFont: xft:Ricty\ for\ Powerline:size=12:antialias=true
URxvt*inputMethod: fcitx
! URxvt*imLocale:    en_US.UTF-8
! URxvt*preeditType: OnTheSpot
urxvt*preeditType: OverTheSpot
! URxvt*preeditType: OffTheSpot
! URxvt*preeditType: Root

! molokai inspired colors (darker version)
! vim: set ft=xdefaults:
URxvt*background:   #323232
URxvt*foreground:   WhiteSmoke
URxvt.transparent:  false

URxvt*cursorColor:  #d0d0d0

URxvt*colorBD:      #3579a8
URxvt*colorUL:      #4eb4fa

!black
URxvt*color0:       #000000
URxvt*color8:       #1c1c1c

!red
URxvt*color1:       #960050
URxvt*color9:       #ff0090

!green
URxvt*color2:       #66aa11
URxvt*color10:      #80ff00

!yellow
URxvt*color3:       #c47f2c
URxvt*color11:      #ffba68

!blue
URxvt*color4:       #3579a8
URxvt*color12:      #5f5fff

!magenta
URxvt*color5:       #7e40a5
URxvt*color13:      #bb88dd

!cyan
URxvt*color6:       #3579a8
URxvt*color14:      #4eb4fa

!white
URxvt*color7:       #5f5f5f
URxvt*color15:      #d0d0d0
